int main() {
    int a, b, N, c;
    int[][] x;
    a = getInt();
    b = getInt();
    N = getInt();

    c = a + b;
    printInt(c);
    x = new int[N][];
    x[0] = new int[N];
    if (N > 0) x[1] = new int[N];
    else {
        x = new int[2][2];
        N = 2;
        x[0] = new int[N];
    }
    if (N > 1) x[2] = new int[N];
    if (N > 2) x[3] = new int[N];
    x[0][0] = a + b;
    printInt(x[0][N - 1]);
}